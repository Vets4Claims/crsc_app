# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Database (Google Cloud PostgreSQL) - Used by Edge Functions
DB_HOST=your-gcp-ip-or-domain
DB_PORT=5432
DB_NAME=crsc_filing
DB_USER=your-db-user
DB_PASSWORD=your-db-password

# Anthropic API - Used by Edge Functions
ANTHROPIC_API_KEY=your-anthropic-key

# Stripe
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-webhook-secret

# Google Cloud Storage (for document uploads) - Used by Edge Functions
GCS_BUCKET_NAME=crsc-documents
GCS_PROJECT_ID=your-gcp-project-id
GCS_CLIENT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GCS_PRIVATE_KEY=your-private-key

# Application
VITE_APP_URL=http://localhost:5173
VITE_PAYMENT_AMOUNT=99.00

# Session Configuration
VITE_SESSION_TIMEOUT_MINUTES=30

# Encryption Key for SSN (Used by Edge Functions)
ENCRYPTION_KEY=your-32-byte-encryption-key
